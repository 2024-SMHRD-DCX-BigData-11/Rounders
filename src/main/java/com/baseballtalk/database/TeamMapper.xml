<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baseballtalk.database.TeamMapper">

	 <select id="selectTeamName" resultType="String" parameterType="int">
	 	select team_name from tbl_team where team_idx = #{team_idx}
	 </select>
	 
	 <select id="selectTeamLogo" resultType="String" parameterType="int">
	 	select team_logo from tbl_team where team_idx = #{team_idx}
	 </select>
</mapper>